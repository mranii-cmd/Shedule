{
  "name": "gestad",
  "version": "0.1.0",
  "type": "module",
  "description": "GestAd - minimal app",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "migrate": "node src/db/runMigrations.js",
    "migrate:seed": "node src/db/runMigrations.js --seed",
    "migrate:rollback": "npx knex migrate:rollback --knexfile src/db/knexfile.js",
    "seed": "knex seed:run --knexfile src/db/knexfile.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec \"node src/index.js\"",
    "lint": "eslint \"src/**/*.js\" \"public/**/*.js\" --ext .js,.mjs",
    "test": "node test/run_tests.js",
    "logs:clear": "rm -rf logs/*.log"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.3",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.2.0",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
